# Backend API Configuration
REACT_APP_BACKEND_URL=http://localhost:8001

# Development Settings
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# WebSocket Settings (for SSE fallback)
WDS_SOCKET_PORT=443

# App Configuration
REACT_APP_NAME=Real-Time Code Editor
REACT_APP_VERSION=1.0.0

# Editor Settings
REACT_APP_DEFAULT_LANGUAGE=javascript
REACT_APP_DEFAULT_THEME=vs-dark

# Features Flags
REACT_APP_ENABLE_DOWNLOAD=true
REACT_APP_ENABLE_COPY_ROOM_ID=true
REACT_APP_ENABLE_CURSORS=true